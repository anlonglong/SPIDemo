<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.aillen.spidemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".fileprovider.FileProviderActivity">
        </activity>
        <activity android:name=".customviewpage.ViewPagerDemoActivity">
        </activity>
        <activity android:name=".CustomActivity">
        </activity>
        <activity android:name=".paintpractise.PaintActivity">
        </activity>
        <activity android:name=".drawtext.DrawTextActivity">
        </activity>
    </application>

</manifest>